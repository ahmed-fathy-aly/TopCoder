ement$TestCase2"/>\n       <array class\="java.lang.Object" length\="5">\n        <void index\="0">\n         <array class\="java.lang.Integer" length\="2">\n          <void index\="0">\n           <int>5000</int>\n          </void>\n          <void index\="1">\n           <int>6500</int>\n          </void>\n         </array>\n        </void>\n        <void index\="1">\n         <array class\="java.lang.Integer" length\="1">\n          <void index\="0">\n           <int>6000</int>\n          </void>\n         </array>\n        </void>\n        <void index\="2">\n         <array class\="java.lang.Integer" length\="1">\n          <void index\="0">\n           <int>6500</int>\n          </void>\n         </array>\n        </void>\n        <void index\="3">\n         <array class\="java.lang.Integer" length\="1">\n          <void index\="0">\n           <int>6000</int>\n          </void>\n         </array>\n        </void>\n        <void index\="4">\n         <array class\="java.lang.Integer" length\="3">\n          <void index\="0">\n           <int>0</int>\n          </void>\n          <void index\="1">\n           <int>5800</int>\n          </void>\n          <void index\="2">\n           <int>6000</int>\n          </void>\n         </array>\n        </void>\n       </array>\n      </void>\n     </void>\n     <void class\="net.fornwall.eclipsecoder.stats.ProblemStatement$TestCase" method\="getField">\n      <string>returnValue</string>\n      <void method\="set">\n       <object idref\="ProblemStatement$TestCase2"/>\n       <int>1</int>\n      </void>\n     </void>\n    </object>\n   </void>\n  </void>\n </object>\n</java>\n
